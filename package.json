{
  "name": "ual-seatmap",
  "version": "0.0.7",
  "description": "Data driven Seatmap",
  "keywords": [
    "seatmap",
    "d3"
  ],
  "license": "MIT",
  "main": "build/ual-seatmap.min.js",
  "jsnext:main": "index",
  "homepage": "https://",
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "config": {
    "testfilter": "test/**/*-test.js"
  },
  "scripts": {
    "test": "tape test/**/*-test.js",
    "test:watch": "gaze \"npm test\" test/**/*-test.js build/**/*.js",
    "build": "rollup -c",
    "build:watch": "npm run build -- -w",
    "bundle": "rollup -c && uglifyjs build/ual-seatmap.js -c -m -o build/ual-seatmap.min.js",
    "dist": "rollup -c && uglifyjs build/ual-seatmap.js -c -m -o dist/ual-seatmap.min.js",
    "prepublish": "npm run bundle",
    "postpublish": "git push && git push --tags",
    "serve": "browser-sync start -s -f *.html build/*.js --reload-delay 1000",
    "start": "npm-run-all -p build:watch test:watch serve"
  },
  "dependencies": {
    "d3": "^4.9.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.12",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gaze-cli": "^0.2.0",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "monaco-editor": "^0.9.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.45",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.0.0",
    "tape": "^4.7.0",
    "uglify-js": "^3.0.24"
  }
}
